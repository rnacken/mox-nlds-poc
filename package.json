{
  "name": "mox-nlds",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --project=unit",
    "generate:css": "ts-node src/moxCss/generateCss.ts",
    "preview": "vite preview",
    "dev:storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "deploy:storybook": "npm run build:storybook && gh-pages -d storybook-static",
    "build:react": "rm -rf ./dist && npm run generate:css && tsc -b tsconfig.components.json && cp -R src/moxCss dist/"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://rnacken.github.io/mox-nlds/",
  "dependencies": {
    "@tabler/icons-react": "^3.36.1",
    "clsx": "^2.1.1",
    "prettier": "^3.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/js": "^9.39.1",
    "@storybook/addon-a11y": "^10.1.4",
    "@storybook/addon-docs": "^10.1.4",
    "@storybook/addon-vitest": "^10.1.4",
    "@storybook/react-vite": "^10.1.4",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.1.4",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "playwright": "^1.57.0",
    "storybook": "^10.1.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.15"
  }
}
